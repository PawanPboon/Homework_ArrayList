<Window x:Class="ArrayList1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArrayList1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="122,0,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="366" Height="50"/>
        <Button x:Name="addData" Content="Add" HorizontalAlignment="Left" Margin="122,259,0,0" VerticalAlignment="Top" Height="41" Width="92" Click="addData_Click"/>
        <Button x:Name="removeData" Content="remove" HorizontalAlignment="Left" Margin="256,259,0,0" VerticalAlignment="Top" Height="41" Width="92" Click="removeData_Click"/>
        <Button x:Name="showData" Content="show" HorizontalAlignment="Left" Margin="378,259,0,0" VerticalAlignment="Top" Height="41" Width="92" Click="showData_Click"/>

    </Grid>
</Window>
